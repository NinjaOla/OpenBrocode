<UserControl x:Class="OpenBrocode.HomeViewUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OpenBrocode"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Ellipse HorizontalAlignment="Left" Margin="11,7,0,0" Stroke="Black" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="300" Height="300">
        <Ellipse.Fill>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF32D34F" Offset="0.007"/>
                <GradientStop Color="#FFF95C5C" Offset="1"/>
                <GradientStop Color="#FF982323" Offset="0.813"/>
                <GradientStop Color="#FF726CF1" Offset="0.75"/>
                <GradientStop Color="#FFA6B515" Offset="0.62"/>
                <GradientStop Color="#FF2D3697" Offset="0.26"/>
            </LinearGradientBrush>
        </Ellipse.Fill>
        <Ellipse.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="17.323"/>
                <TranslateTransform/>
            </TransformGroup>
        </Ellipse.RenderTransform>
    </Ellipse>

</UserControl>
